  /* Display: Content pane */
  $handler = $view->new_display('panel_pane', 'Content pane', 'sidebar_pane');
  $handler->display->display_options['defaults']['hide_admin_links'] = FALSE;
  $translatables['next_events'] = array(
    t('Master'),
    t('Next events'),
    t('more'),
    t('Apply'),
    t('Reset'),
